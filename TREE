#include<stdio.h>
#include<stdlib.h>
struct Node{
    int data;
    struct Node* left;
    struct Node* right;
};
int main(){
    struct Node* root=malloc(sizeof(struct Node));
    struct Node* leftchild=malloc(sizeof(struct Node));
    struct Node* rightchild=malloc(sizeof(struct Node));
    root->data=1;
    leftchild->data=2;
    rightchild->data=3;
    root->left=leftchild;
    root->right=rightchild;
    leftchild->left=leftchild->right;
    rightchild->right=rightchild->left;
    printf("root:%d\n",root->data);
    printf("leftchild:%d\n",root->left->data);
    printf("rightchild:%d\n",root->right->data);
    return 0;
}
