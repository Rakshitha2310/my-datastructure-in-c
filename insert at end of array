//insert at end of the array
#include<stdio.h>
#define CAP 10
int main(){
    int arr[]={1,2,3,4,5};
    int n=12;
    int value=6;
    if(n<CAP){
        arr[n]=value;
        n++;
        for(int i=0;i<n;i++){
            printf("%d ",arr[i]);
    }
        
    }
    else{
        printf("array is full\n");
    }
}
