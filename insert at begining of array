//insert at begining of the array
#include<stdio.h>
#define CAP 5
int main(){
    int a[CAP]={1,2,3,4,5};
    int n=4;
    int value=0;
    if(n<CAP){
       for(int i=0;i>0;i--){
           a[i]=a[i-1];
       }
       a[0]=value;
       n++;
    }
    else{
        printf("array is full");
    }
    for(int i=0;i<n;i++){
        printf("%d ",a[i]);
    }
}
       
