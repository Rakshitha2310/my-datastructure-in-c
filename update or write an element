//update/write an element
#include<stdio.h>
int main(){
    int arr[]={2,3,4,6,5,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int index = 3;
    int newvalue = 9;
    if(index>=0 && index<n){
        arr[index]=newvalue;
        for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }
    }
    else
    printf("out of order\n");
}
